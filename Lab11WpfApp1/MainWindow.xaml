<Window x:Class="Lab11WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab11WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="400">
    <Grid>
        <Path Fill="Red" Stroke="Black" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Path.Data>
                <GeometryGroup>
                    <RectangleGeometry Rect="20,110,180,20">
                        <RectangleGeometry.Transform>
                            <RotateTransform Angle="-45" CenterX="32" CenterY="150"/>
                        </RectangleGeometry.Transform>
                    </RectangleGeometry>
                </GeometryGroup>
            </Path.Data>
        </Path>
        <Path Fill="Red" Stroke="Black"  VerticalAlignment="Center" HorizontalAlignment="Center">
            <Path.Data>
                <GeometryGroup >
                    <EllipseGeometry RadiusX="100" RadiusY="100" Center="100,100"/>
                    <EllipseGeometry RadiusX="80" RadiusY="80" Center="100,100"/>
                </GeometryGroup>
            </Path.Data>
        </Path>
    </Grid>
</Window>
